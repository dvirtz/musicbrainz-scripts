{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "//#lint:root": {},
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "env": [
        "MB_USERNAME",
        "MB_PASSWORD"
      ]
    }
  }
}
